<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#e8e8f9"
    tools:context=".FindDoctorActivity">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="-32dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingBottom="10dp">

                <ImageView
                    android:id="@+id/imageView2"
                    android:layout_width="405dp"
                    android:layout_height="127dp"
                    app:layout_constraintBottom_toBottomOf="@+id/imageView"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="@+id/imageView"
                    app:layout_constraintTop_toTopOf="@+id/imageView"
                    app:srcCompat="@drawable/top_background" />

                <ImageView
                    android:id="@+id/imageView"
                    android:layout_width="411dp"
                    android:layout_height="149dp"
                    android:scaleType="fitXY"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:srcCompat="@drawable/top_background4" />

                <ImageView
                    android:id="@+id/imageView3"
                    android:layout_width="103dp"
                    android:layout_height="90dp"
                    android:layout_marginEnd="28dp"
                    app:layout_constraintBottom_toBottomOf="@+id/imageView2"
                    app:layout_constraintEnd_toEndOf="@+id/imageView2"
                    app:layout_constraintTop_toTopOf="@+id/imageView2"
                    app:layout_constraintVertical_bias="1.0"
                    app:srcCompat="@drawable/profile" />

                <TextView
                    android:id="@+id/textView2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="32dp"
                    android:text="HealThcare"
                    android:textColor="#ffffff"
                    android:textSize="18sp"
                    android:textStyle="bold"
                    app:layout_constraintStart_toStartOf="@+id/imageView2"
                    app:layout_constraintTop_toTopOf="@+id/imageView3" />

                <TextView
                    android:id="@+id/textView3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:text="TOP DOCTORS"
                    android:textColor="#ffffff"
                    android:textSize="25sp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="@+id/imageView3"
                    app:layout_constraintStart_toStartOf="@+id/textView2"
                    app:layout_constraintTop_toBottomOf="@+id/textView2"
                    app:layout_constraintVertical_bias="0.562" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <EditText
                android:id="@+id/editTextTextPersonName2"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginStart="40dp"
                android:layout_marginTop="16dp"
                android:layout_marginEnd="40dp"
                android:background="@drawable/background_edittext_search"
                android:drawableEnd="@drawable/search_icon"
                android:ems="10"
                android:hint="Search for..."
                android:inputType="textPersonName"
                android:paddingStart="8dp"
                android:paddingEnd="4dp"
                android:textColor="#8D8D8D"
                android:textSize="16dp"
                android:textStyle="bold|italic" />


            <LinearLayout
                android:layout_width="346dp"
                android:layout_height="match_parent"
                android:layout_marginStart="30dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="30dp"
                android:orientation="vertical"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="0"
                        android:orientation="vertical"
                        tools:layout_margin="10dp">

                        <androidx.cardview.widget.CardView
                            android:id="@+id/cardFDFamilyPhysician"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:background="@drawable/white_top_buttons">

                            <ImageView
                                android:layout_width="117dp"
                                android:layout_height="107dp"
                                android:layout_marginStart="16dp"
                                android:layout_marginTop="16dp"
                                android:layout_marginEnd="16dp"
                                android:layout_marginBottom="16dp"
                                android:background="@drawable/white_top_buttons"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent"
                                app:srcCompat="@drawable/family_physicians"
                                tools:ignore="DuplicateIds" />
                        </androidx.cardview.widget.CardView>

                        <TextView
                            android:id="@+id/textView7"
                            android:layout_width="80dp"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="8dp"
                            android:text="Lab Test"
                            android:textAlignment="center"
                            android:textColor="#2e3d6d"
                            android:textSize="14dp"
                            android:textStyle="bold"
                            tools:ignore="DuplicateIds,TextSizeCheck" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="0"
                        android:orientation="vertical"
                        tools:layout_margin="10dp"
                        android:layout_marginLeft="20dp">

                        <androidx.cardview.widget.CardView
                            android:id="@+id/cardFDDietician"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:background="@drawable/white_top_buttons">

                            <ImageView

                                android:layout_width="127dp"
                                android:layout_height="107dp"
                                android:layout_marginStart="16dp"
                                android:layout_marginTop="16dp"
                                android:layout_marginEnd="16dp"
                                android:layout_marginBottom="16dp"
                                android:background="@drawable/white_top_buttons"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent"
                                app:srcCompat="@drawable/dietician"
                                tools:ignore="DuplicateIds" />
                        </androidx.cardview.widget.CardView>

                        <TextView
                            android:id="@+id/textView7"
                            android:layout_width="80dp"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="8dp"
                            android:text="Medicine"
                            android:textAlignment="center"
                            android:textColor="#2e3d6d"
                            android:textSize="14dp"
                            android:textStyle="bold"
                            tools:ignore="DuplicateIds,TextSizeCheck" />
                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="156dp"
                        android:layout_weight="0"
                        android:orientation="vertical"
                        tools:layout_margin="10dp">

                        <androidx.cardview.widget.CardView
                            android:id="@+id/cardFDDentist"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:background="@drawable/white_top_buttons">

                            <ImageView

                                android:layout_width="115dp"
                                android:layout_height="91dp"
                                android:layout_marginStart="16dp"
                                android:layout_marginTop="16dp"
                                android:layout_marginEnd="16dp"
                                android:layout_marginBottom="16dp"
                                android:background="@drawable/white_top_buttons"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent"
                                app:srcCompat="@drawable/dentist"
                                tools:ignore="DuplicateIds" />
                        </androidx.cardview.widget.CardView>

                        <TextView
                            android:id="@+id/textView7"
                            android:layout_width="80dp"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="8dp"
                            android:text="Calendar"
                            android:textAlignment="center"
                            android:textColor="#2e3d6d"
                            android:textSize="14dp"
                            android:textStyle="bold"
                            tools:ignore="DuplicateIds,TextSizeCheck" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="156dp"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        tools:layout_margin="10dp"
                        android:layout_marginLeft="20dp">

                        <androidx.cardview.widget.CardView
                            android:id="@+id/cardFDSurgenon"
                            android:layout_width="wrap_content"
                            android:layout_height="126dp"
                            android:background="@drawable/white_top_buttons">

                            <ImageView

                                android:layout_width="127dp"
                                android:layout_height="110dp"
                                android:layout_marginStart="16dp"
                                android:layout_marginTop="16dp"
                                android:layout_marginEnd="16dp"
                                android:layout_marginBottom="16dp"
                                android:background="@drawable/white_top_buttons"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent"
                                app:srcCompat="@drawable/surgeon"
                                tools:ignore="DuplicateIds" />
                        </androidx.cardview.widget.CardView>

                        <TextView
                            android:id="@+id/textView7"
                            android:layout_width="80dp"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="8dp"
                            android:text="Order"
                            android:textAlignment="center"
                            android:textColor="#2e3d6d"
                            android:textSize="14dp"
                            android:textStyle="bold"
                            tools:ignore="DuplicateIds,TextSizeCheck" />
                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="138dp"
                        android:layout_weight="0"
                        android:orientation="vertical"
                        tools:layout_margin="10dp">

                        <androidx.cardview.widget.CardView
                            android:id="@+id/cardFDCarddiologists"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:background="@drawable/white_top_buttons"
                            tools:ignore="DuplicateIds">

                            <ImageView

                                android:layout_width="112dp"
                                android:layout_height="91dp"
                                android:layout_marginStart="16dp"
                                android:layout_marginTop="16dp"
                                android:layout_marginEnd="16dp"
                                android:layout_marginBottom="16dp"
                                android:background="@drawable/white_top_buttons"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent"
                                app:srcCompat="@drawable/cardiologists"
                                tools:ignore="DuplicateIds" />
                        </androidx.cardview.widget.CardView>

                        <TextView
                            android:id="@+id/textView7"
                            android:layout_width="80dp"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="8dp"
                            android:text="Calendar"
                            android:textAlignment="center"
                            android:textColor="#2e3d6d"
                            android:textSize="14dp"
                            android:textStyle="bold"
                            tools:ignore="DuplicateIds,TextSizeCheck" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="0"
                        android:orientation="vertical"
                        tools:layout_margin="10dp">

                        <androidx.cardview.widget.CardView
                            android:id="@+id/cardFDBack"
                            android:layout_width="158dp"
                            android:layout_height="wrap_content"
                            android:background="@drawable/white_top_buttons"
                            tools:ignore="DuplicateIds">

                            <ImageView

                                android:layout_width="125dp"
                                android:layout_height="80dp"
                                android:layout_marginStart="16dp"
                                android:layout_marginTop="16dp"
                                android:layout_marginEnd="16dp"
                                android:layout_marginBottom="16dp"
                                android:background="@drawable/white_top_buttons"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent"
                                app:srcCompat="@drawable/info"
                                tools:ignore="DuplicateIds" />
                        </androidx.cardview.widget.CardView>

                        <TextView
                            android:id="@+id/textView7"
                            android:layout_width="80dp"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="8dp"
                            android:text="Back"
                            android:textAlignment="center"
                            android:textColor="#2e3d6d"
                            android:textSize="14dp"
                            android:textStyle="bold"
                            tools:ignore="DuplicateIds,TextSizeCheck" />
                    </LinearLayout>

                </LinearLayout>
            </LinearLayout>

        </LinearLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>